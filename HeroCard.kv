<HeroCard_Nutrition>
    orientation: 'vertical'
    name:'once'
    size_hint: 1,None
    height: "320dp"
    elevation: 8
    radius: 24
    padding: 12

    MDHeroFrom:
        id: hero_from
        tag: root.tag
        size_hint: 1, None
        height: root.height / 1.8

        FitImage:
            source: root.source
            size_hint: None,None
            size: hero_from.size
            radius: 24

    MDLabel:
        text:'Питание'
        bold: True
        adaptive_height: True
        padding: 12,12
        font_style: 'H5'

    MDLabel:
        id: eat
        padding: 12,12
        text:'Ешьте здоровую пищу. Вы - то, что вы едите.'



