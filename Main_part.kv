<FiveWindow>:
    name: 'five'
    id: log

    FitImage:
        source:"bg.png"

    BoxLayout:
        size_hint: 1,1
        pos_hint: {'center_x':.5, 'center_y':.5}
        padding: 25
        spacing: 25
        orientation: 'vertical'

        MDLabel:
            id: welcome_label
            text: 'Пожалуйста, укажите свои данные: '
            font_size: 25
            halign: 'center'
            size_hint_y: None
            height: self.texture_size[1]
            padding_y: 15

        MDTextField:
            id: firstname
            hint_text: "Имя"
            helper_text_mode: 'on_focus'
            line_color_normal: 'black'
            line_color_focus: "black"
            hint_text_color_normal: "black"
            hint_text_color_focus: "black"
            text_color_focus: "black"
            icon_right: 'account'
            icon_right_color_normal: "black"
            icon_right_color_focus: "black"
            size_hint_x: None
            width: 200
            font_size: 20
            pos_hint: {'center_x':.5}

        MDTextField:
            id: secondname
            hint_text: "Фамилия"
            helper_text_mode: 'on_focus'
            line_color_normal: 'black'
            line_color_focus: "black"
            hint_text_color_normal: "black"
            hint_text_color_focus: "black"
            text_color_focus: "black"
            icon_right: 'account'
            icon_right_color_normal: "black"
            icon_right_color_focus: "black"
            size_hint_x: None
            width: 200
            font_size: 20
            pos_hint: {'center_x':.5}

        GridLayout:
            cols:2

            Label:
                color: (7/255,7/255,7/255,1)
                text: "М"
                font_size: 18

            Label:
                color: (7/255,7/255,7/255,1)
                text: " Ж"
                font_size: 18

            CheckBox:
                color: (7/255,7/255,7/255,1)
                id: check
                group: 'sex'
                on_active: app.checkbox_click(self, self.active, "male")

            CheckBox:
                color: (7/255,7/255,7/255,1)
                id: check2
                group: 'sex'
                on_active: app.checkbox_click(self, self.active, "female")


        BoxLayout:
            size_hint: 1,1

            Widget:

            MDRoundFlatButton:
                text:'Далее'
                text_color: "black"
                line_color: "black"
                font_size: 12
                padding:10
                on_press: app.logger()

                on_release:
                    app.root.current = "six"
                    root.manager.transition.direction = "left"

            Widget:
                size_hint:.1,.1

            MDRoundFlatButton:
                text:'Очистить'
                text_color: "black"
                line_color: "black"
                font_size: 12
                padding:3
                on_press: app.clear_five_window()

            Widget:

        Widget:
            size_hint:.1,.1





